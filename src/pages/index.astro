---
import Layout from "../layouts/Layout.astro";
import { MainFormWrapper } from "../wrappers";
import AboutWrapper from "../wrappers/About.wrapper";
import HeroCarouselWrapper from "../wrappers/HeroCarousel.wrapper";
import PricingWrapper from "../wrappers/Pricing.wrapper";
import CardWrapper from "../wrappers/ServiceCard.wrapper";
import TestimonialCarouselWrapper from "../wrappers/TestimonialCarouselWrapper";
---

<Layout>
	<HeroCarouselWrapper client:load />
	<AboutWrapper client:load />
	<CardWrapper client:load />
	<PricingWrapper client:load />
	<TestimonialCarouselWrapper client:load />
	<MainFormWrapper client:load formId="contact" />
</Layout>
